---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>SJackson</title>
  <link href="http://sjackson.net/feed/" rel="self"/>
  <link href="http://sjackson.net"/>

  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://sjackson.net/</id>
  <author>
    <name>Steve Jackson</name>
    <email>steven.j.jackson@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://sjackson.net/{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://sjackson.net/{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
